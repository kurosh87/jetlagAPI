{
  "name": "jetlag-api",
  "version": "1.0.0",
  "description": "A robust TypeScript API service for jetlag mitigation",
  "main": "dist/api/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/api/index.ts",
    "dev:emulator": "firebase emulators:start",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:emulator\"",
    "build": "tsc",
    "start": "ts-node src/app.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "deploy": "firebase deploy",
    "deploy:rules": "firebase deploy --only firestore:rules,storage:rules"
  },
  "keywords": [
    "jetlag",
    "travel",
    "api",
    "typescript",
    "circadian-rhythm"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@firebasegen/default-connector": "file:dataconnect-generated/js/default-connector",
    "@types/compression": "^1.7.5",
    "@types/helmet": "^4.0.0",
    "@types/luxon": "^3.4.2",
    "@vercel/node": "^2.9.6",
    "amadeus": "^11.0.0",
    "axios": "^1.7.9",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "firebase-admin": "^11.5.0",
    "helmet": "^8.0.0",
    "luxon": "^3.5.0",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "redis": "^4.7.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^22.10.6",
    "@types/node-fetch": "^2.6.12",
    "@types/uuid": "^10.0.0",
    "concurrently": "^8.2.2",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  }
}
